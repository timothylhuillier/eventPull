{
  "name": "Sample - OAuth Contacts",
  "version": "1.0.6",
  "icons": { "48": "img/icon-48.png",
            "128": "img/icon-128.png" },
  "description": "Uses OAuth to connect to Google's contacts service and display a list of your contacts.",
  "background": {
    "scripts": [
      "chrome_ex_oauthsimple.js",
      "chrome_ex_oauth.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "img/icon-19-off.png",
    "default_popup": "pop_up.html"
  },
  "permissions": [
    "tabs",
    "http://www.google.com/m8/feeds/*",
    "https://www.google.com/accounts/OAuthGetRequestToken",
    "https://www.google.com/accounts/OAuthAuthorizeToken",
    "https://www.google.com/accounts/OAuthGetAccessToken",
    "http://ec2-54-226-248-250.compute-1.amazonaws.com:8080/sms",
    "http://ec2-54-226-248-250.compute-1.amazonaws.com:8080/"

  ],
  "manifest_version": 2
}
